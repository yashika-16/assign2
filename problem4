#include <iostream>
using namespace std;


void rotateMatrix(int mat[][100], int n) {
    for (int i=0;i<n;i++) { 
        for(int j=i;j<n;j++) {
            swap(mat[i][j], mat[j][i]);    
        }   
    }
    
    for(int i=0;i<n;i++) {
        int top=0, bottom=n-1;
        while(top<bottom) {
            swap(mat[bottom][i],mat[top][i]);
            top++;
            bottom--;
        }
    }
    
    for(int i=0;i<n;i++) {
        for(int j=0;j<n;j++) {
            cout<<mat[i][j]<<" ";
        }
        cout<<endl;
    }
}

int main() {
    int n;
    int mat[100][100];
    cin>>n;
    for(int i=0;i<n;i++) {
        for(int j=0;j<n;j++) {
            cin>>mat[i][j];
        }
    }
    rotateMatrix(mat,n);
    return 0;
}
